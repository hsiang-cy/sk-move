{
  "name": "dispatch",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "imports": {
    "#index": "./src/index.ts",
    "#db": "./src/db/drizzleORM.export.ts",
    "#route/*": "./src/route/*",
    "#middleware": "./src/utils/middleware/index.ts",
    "#types": "./src/utils/types.ts",
    "#factory": "./src/utils/factory.ts",
    "#helpers/*": "./src/utils/helpers/*",
    "#root/*": "./src/*"
  },
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy --minify",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "type": "pnpm exec tsc --watch",
    "generate": "drizzle-kit generate --config=src/db/drizzle.config.ts",
    "migrate": "drizzle-kit migrate --config=src/db/drizzle.config.ts",
    "migrate:undo": "drizzle-kit migrate:undo --config=src/db/drizzle.config.ts",
    "migrate:status": "drizzle-kit migrate:status --config=src/db/drizzle.config.ts",
    "test": "vitest"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "drizzle-kit": "^0.31.9",
    "tsx": "^4.20.6",
    "vitest": "^4.0.15",
    "wrangler": "^4.67.0"
  },
  "dependencies": {
    "@hono/standard-validator": "^0.2.0",
    "@hono/typebox-validator": "^1.1.0",
    "@hono/zod-validator": "^0.7.6",
    "@neondatabase/serverless": "^1.0.2",
    "@scalar/hono-api-reference": "^0.9.25",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "hono": "^4.12.0",
    "typescript": "^5.9.3"
  }
}
